{
  "name": "@jcdubs/janus",
  "version": "0.0.1",
  "description": "Open source Serverless authentication: A Cedar-based authorisation engine for deterministic, deny-by-default access decisions through a CDK construct and SDK libraries.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "vendor"
  ],
  "scripts": {
    "build": "rm -f tsconfig.tsbuildinfo && rm -rf ./dist && tsc",
    "build:vendor": "rm -rf ./vendor && sh ./scripts/copy-assets.sh",
    "commit": "cz",
    "dev:setup": "pnpx lefthook install",
    "format:check": "biome check --formatter-enabled=true --linter-enabled=false --write .",
    "format:fix": "biome format --write --fix .",
    "format": "biome format --write .",
    "lint": "biome check --linter-enabled=true --formatter-enabled=false .",
    "lint:fix": "biome check --linter-enabled=true --formatter-enabled=true --write .",
    "preinstall": "npx only-allow pnpm",
    "prepack": "npm run build",
    "prepublishOnly": "npm test",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "keywords": [
    "serverless",
    "authentication",
    "cdk",
    "sdk",
    "typescript",
    "aws",
    "open-source"
  ],
  "author": "",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JCDubs/Janus.git"
  },
  "bugs": {
    "url": "https://github.com/JCDubs/Janus/issues"
  },
  "homepage": "https://github.com/JCDubs/Janus#readme",
  "license": "MIT",
  "packageManager": "pnpm@10.15.1",
  "devDependencies": {
    "@aws-lambda-powertools/logger": "2.15.0",
    "@biomejs/biome": "^2.3.10",
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@middy/core": "^6.4.5",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@swc/jest": "0.2.39",
    "@types/aws-lambda": "^8.10.159",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "aws-cdk-lib": "2.219.0",
    "commit-and-tag-version": "12.6.0",
    "commitizen": "4.3.1",
    "constructs": "10.4.2",
    "cz-conventional-changelog": "3.3.0",
    "jest": "^30.2.0",
    "lefthook": "1.13.6",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@cedar-policy/cedar-wasm": "4.3.3",
    "uuid": "^13.0.0"
  },
  "peerDependencies": {
    "@aws-lambda-powertools/logger": "2.15.0",
    "aws-cdk-lib": "2.219.0",
    "constructs": "10.4.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "tag": "latest",
    "access": "public",
    "provenance": true
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=10"
  }
}